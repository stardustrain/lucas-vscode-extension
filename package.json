{
  "name": "lucas-helper",
  "displayName": "LucasHelper",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "test"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lucasHelper.makeFunction",
        "title": "lucas: Make arrow function"
      },
      {
        "command": "lucasHelper.addNewTsFile",
        "title": "lucas: Add a new file"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish",
    "vscode:prepublish": "npm run compile",
    "build": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --target=es2020 --format=cjs --platform=node",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "@vscode/vsce": "^2.21.1",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "change-case-all": "^1.0.15"
  }
}
